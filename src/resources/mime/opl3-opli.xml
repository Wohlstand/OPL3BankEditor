<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-opl3-opli">
    <sub-class-of type="application/octet-stream"/>
    <icon name="x-application-opli-instrument"/>
    <comment>OPL3 instrument (OPLI)</comment>
    <magic priority="50">
        <match type="string" offset="0" value="WOPL3-INST\0"/>
    </magic>
    <glob pattern="*.opli"/>
  </mime-type>
</mime-info>
